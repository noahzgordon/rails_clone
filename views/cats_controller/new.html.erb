<%= flash[:error] %>

<form action="<%= cats_url %>" method="POST">
  <input type="hidden" name="authenticity_token"
         value="<%= form_authenticity_token %>">

  <label>Name
    <input type="text" name="cat[name]" value="<%= @cat.name %>">
  </label>

  <label for="cat-owner">Owner</label>
  <select name="cat[owner_id]" id="cat-owner">
		<% Human.all.each do |human| %>
			<option value="<%= human.id %>"><%= human.fname %></option>
		<% end %>
	</select>

  <input type="submit">
</form>
